{
  "main": "lib/index.js",
  "type": "commonjs",
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "ci-test": "jest",
    "format:write": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "package": "ncc build src/index.ts --license licenses.txt && npm run create-wrappers",
    "create-wrappers": "node scripts/create-wrappers.js",
    "package:watch": "npm run package -- --watch",
    "test": "jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run package"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.1",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.2.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.7.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@actions/glob": "^0.5.0",
    "@actions/artifact": "^4.0.0",
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@octokit/core": "^7.0.6",
    "@octokit/graphql": "^9.0.3",
    "@octokit/rest": "^22.0.1"
  },
  "overrides": {
    "glob": "^10.5.0"
  }
}
